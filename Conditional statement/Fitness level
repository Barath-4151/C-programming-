#include <stdio.h>
int main() {
    int steps, time;
    printf("Enter steps and time(min): ");
    scanf("%d%d",&steps,&time);

    if(steps >= 10000 && time <= 60)
        printf("Excellent Fitness");
    else if(steps >= 5000)
        printf("Average Fitness");
    else
        printf("Low Fitness");

    return 0;
}
