#include <stdio.h>
int main() {
    int age;
    printf("Enter age: ");
    scanf("%d",&age);

    if(age < 0)
        printf("Invalid Age");
    else if(age <= 5)
        printf("Mashed vegetables or soft fruits");
    else if(age <= 12)
        printf("Milk and healthy snacks");
    else if(age <= 18)
        printf("Burgers or pasta");
    else if(age <= 50)
        printf("Rice, vegetables, protein food");
    else
        printf("Soup or grilled fish");

    return 0;
}
