#include <stdio.h>

int factorial(int n) {
    int fact = 1, i;
    for(i = 1; i <= n; i++)
        fact *= i;
    return fact;
}

int main() {
    int n, temp, sum = 0, rem;

    scanf("%d", &n);
    temp = n;

    while(n != 0) {
        rem = n % 10;
        sum += factorial(rem);
        n /= 10;
    }

    if(sum == temp)
        printf("Strong Number");
    else
        printf("Not Strong Number");

    return 0;
}
