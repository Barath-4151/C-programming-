#include <stdio.h>
#include <math.h>
int main() {
    float angle,x,y,newx,newy;
    char dir;
    scanf("%f %c %f %f",&angle,&dir,&x,&y);

    angle = angle * 3.14159 / 180;

    if(dir=='C') angle = -angle;

    newx = x*cos(angle) - y*sin(angle);
    newy = x*sin(angle) + y*cos(angle);

    printf("New Coordinates: %.2f %.2f",newx,newy);
    return 0;
}
