#include <stdio.h>
int main() {
    float salary, bonus, final;

    printf("Enter salary: ");
    scanf("%f",&salary);

    if(salary >= 50000)
        bonus = salary * 0.10;
    else
        bonus = salary * 0.05;

    final = salary + bonus;

    printf("Final Salary = %.2f", final);
    return 0;
}
