#include <stdio.h>
int main() {
    int hours;
    char gender;
    float salary=0;

    printf("Enter hours and gender(M/F): ");
    scanf("%d %c",&hours,&gender);

    if(hours <= 8){
        if(gender=='M')
            salary = hours * 80;
        else
            salary = hours * 60;
    }
    else{
        salary = 8 * ((gender=='M')?80:60);
        salary += (hours-8) * 100;
    }

    printf("Total Salary = %.2f",salary);
    return 0;
}
