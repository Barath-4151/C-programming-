#include <stdio.h>
int main() {
    int m;
    printf("Enter month (1-12): ");
    scanf("%d",&m);

    if(m==1||m==3||m==5||m==7||m==8||m==10||m==12)
        printf("31 Days\n");
    else if(m==4||m==6||m==9||m==11)
        printf("30 Days\n");
    else if(m==2)
        printf("28 or 29 Days\n");
    else
        printf("Invalid Month\n");

    if(m>=1 && m<=3) printf("1st Quadrant");
    else if(m<=6) printf("2nd Quadrant");
    else if(m<=9) printf("3rd Quadrant");
    else if(m<=12) printf("4th Quadrant");

    return 0;
}
